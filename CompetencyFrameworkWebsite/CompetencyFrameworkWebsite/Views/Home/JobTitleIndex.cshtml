@model CompetencyFrameworkWebsite.Models.JobTitleModel
@{
    ViewBag.Title = "Job Title";
}

<form method="post" action="">
    <div>
        <select>
        @{
            <div style="padding: 10px">
                @Html.DropDownList("Jobtitles", new SelectList(Model.JobTitles))
                <input type="submit" class="btn btn-default" value="Select">
            </div>
          }
        </select>
    </div>
</form>